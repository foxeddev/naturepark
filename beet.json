{
  "id": "naturepark",
  "name": "Nature Park",
  "version": "1.0.0",
  "description": "",
  "author": "Rotsteinfuchs",
  "output": "build",
  "require": ["bolt"],
  "data_pack": {
    "load": ["src"],
    "render": {
      "functions": ["*"]
    }
  },
  "resource_pack": {
    "load": ["src"]
  },
  "pipeline": [
    "build.main",
    "plugins.function_tags",
    "plugins.extend.installation_advancement.pack_logo",
    "beet.contrib.installation_advancement",
    "beet.contrib.relative_function_path",
    "mecha",
    "beet.contrib.minify_function",
    "beet.contrib.minify_json"
  ],
  "meta": {
    "default_namespace": "naturepark",
    "project_namespaces": ["naturepark"]
  }
}
